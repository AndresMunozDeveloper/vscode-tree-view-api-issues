{
  "name": "vscode-tree-view-api-issues",
  "displayName": "vscode-tree-view-api-issues",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "treeviewissues",
          "name": "Tree View Issues"
        }
      ]
    },
    "commands": [
      {
        "command": "treeviewissues.commandForAnyNumberOfSelectedNodes",
        "title": "Command For Any Number Of Selected Nodes",
        "icon": "$(account)"
      },
      {
        "command": "treeviewissues.commandForExactlyOneSelectedNode",
        "title": "Command For Exactly One Selected Node",
        "icon": "$(activate-breakpoints)",
        "enablement": "treeviewissues.selectedNodeCount == 1"
      },
      {
        "command": "treeviewissues.commandForOneOrMoreSelectedNodes",
        "title": "Command For One Or More Selected Nodes",
        "icon": "$(add)",
        "enablement": "treeviewissues.selectedNodeCount >= 1"
      },
      {
        "command": "treeviewissues.collapseAllNodes",
        "title": "Collapse all nodes",
        "icon": "$(collapse-all)"
      },
      {
        "command": "treeviewissues.expandAllNodes",
        "title": "Expand all nodes",
        "icon": "$(expand-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "treeviewissues.commandForAnyNumberOfSelectedNodes",
          "when": "view === treeviewissues",
          "group": "navigation@1"
        },
        {
          "command": "treeviewissues.commandForExactlyOneSelectedNode",
          "when": "view === treeviewissues",
          "group": "navigation@2"
        },
        {
          "command": "treeviewissues.commandForOneOrMoreSelectedNodes",
          "when": "view === treeviewissues",
          "group": "navigation@3"
        },
        {
          "command": "treeviewissues.collapseAllNodes",
          "when": "view === treeviewissues",
          "group": "navigation@4"
        },
        {
          "command": "treeviewissues.expandAllNodes",
          "when": "view === treeviewissues",
          "group": "navigation@5"
        },
        {
          "command": "treeviewissues.commandForAnyNumberOfSelectedNodes",
          "when": "view === treeviewissues",
          "group": "1@1"
        },
        {
          "command": "treeviewissues.commandForExactlyOneSelectedNode",
          "when": "view === treeviewissues",
          "group": "1@2"
        },
        {
          "command": "treeviewissues.commandForOneOrMoreSelectedNodes",
          "when": "view === treeviewissues",
          "group": "1@3"
        }
      ],
      "view/item/context": [
        {
          "command": "treeviewissues.commandForAnyNumberOfSelectedNodes",
          "when": "view === treeviewissues",
          "group": "1@1"
        },
        {
          "command": "treeviewissues.commandForOneOrMoreSelectedNodes",
          "when": "view === treeviewissues",
          "group": "1@2"
        },
        {
          "command": "treeviewissues.commandForExactlyOneSelectedNode",
          "when": "view === treeviewissues",
          "group": "1@3"
        }
      ]
    },
    "keybindings": [
      {
        "command": "treeviewissues.commandForAnyNumberOfSelectedNodes",
        "key": "alt+0",
        "when": "focusedView == treeviewissues"
      },
      {
        "command": "treeviewissues.commandForExactlyOneSelectedNode",
        "key": "alt+1",
        "when": "focusedView == treeviewissues"
      },
      {
        "command": "treeviewissues.commandForOneOrMoreSelectedNodes",
        "key": "alt+2",
        "when": "focusedView == treeviewissues"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
