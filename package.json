{
  "name": "vscode-tree-view-api-issues",
  "displayName": "vscode-tree-view-api-issues",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "treeviewissues",
          "name": "Tree View Issues"
        }
      ]
    },
    "commands": [
      {
        "command": "treeviewissues.commandThatDoesNotRequireASelectedString",
        "title": "Command That Does Not Require A Selected String",
        "icon": "$(account)"
      },
      {
        "command": "treeviewissues.commandThatRequiresAtLeastOneSelectedString",
        "title": "Command That Requires At Least One Selected String",
        "icon": "$(activate-breakpoints)",
        "enablement": "treeviewissues.selectedTreeItemCount >= 1"
      },
      {
        "command": "treeviewissues.commandThatRequiresExactlyOneSelectedString",
        "title": "Command That Requires Exactly One Selected String",
        "icon": "$(add)",
        "enablement": "treeviewissues.selectedTreeItemCount == 1"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "treeviewissues.commandThatDoesNotRequireASelectedString",
          "when": "view === treeviewissues",
          "group": "navigation@1"
        },
        {
          "command": "treeviewissues.commandThatRequiresAtLeastOneSelectedString",
          "when": "view === treeviewissues",
          "group": "navigation@2"
        },
        {
          "command": "treeviewissues.commandThatRequiresExactlyOneSelectedString",
          "when": "view === treeviewissues",
          "group": "navigation@3"
        },
        {
          "command": "treeviewissues.commandThatDoesNotRequireASelectedString",
          "when": "view === treeviewissues",
          "group": "1@1"
        },
        {
          "command": "treeviewissues.commandThatRequiresAtLeastOneSelectedString",
          "when": "view === treeviewissues",
          "group": "1@2"
        },
        {
          "command": "treeviewissues.commandThatRequiresExactlyOneSelectedString",
          "when": "view === treeviewissues",
          "group": "1@3"
        }
      ],
      "view/item/context": [
        {
          "command": "treeviewissues.commandThatDoesNotRequireASelectedString",
          "when": "view === treeviewissues"
        },
        {
          "command": "treeviewissues.commandThatRequiresAtLeastOneSelectedString",
          "when": "view === treeviewissues"
        },
        {
          "command": "treeviewissues.commandThatRequiresExactlyOneSelectedString",
          "when": "view === treeviewissues"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
